{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node",
			"config": { "buildCommand": "npm run build" }
		}
	],
	"routes": [
		{
			"src": "/assets/(.*)",
			"dest": "/dist/client/assets/$1"
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"dest": "/api/server.js"
		}
	]
}
